FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    air_inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 300;
    }
}


