FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 101325;
    }
    sides
    {
        type            calculated;
        value           uniform 101325;
    }
    air_inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 
0
;
    }
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 
0
;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 101325;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 101325;
    }
}


